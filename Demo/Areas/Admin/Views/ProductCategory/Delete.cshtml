@model Demo.Data.Category



@{
    ViewData["Title"] = "Delete";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}
<div class="content_box">
    <dl class="form_container">
        <div class="title">Category</div>
        <div class="content">
            <div class="user-details">
                <div class="input-box">
                    <label>Category Name</label>
                    <input asp-for="CatName" class="form-control" readonly />
                </div>

                <div class="input-box">
                    <label asp-for="Description">Description</label>
                    <input asp-for="Description" class="form-control" readonly />
                </div>

                <div class="input-box">
                    <label asp-for="ParentId">ParentId</label>
                    <input asp-for="ParentId" class="form-control" readonly />
                </div>

                <div class="input-box">
                    <label asp-for="Levels">Levels</label>
                    <input asp-for="Levels" class="form-control" readonly />
                </div>

                <div class="input-box">
                    <label asp-for="Ordering">Create Date</label>
                    <input asp-for="Ordering" class="form-control" readonly />
                </div>

                <div class="input-box">
                    <label asp-for="Published" class="control-label">Published</label>
                    <input asp-for="Published" class="form-control" readonly />
                </div>

                <div class="input-box">
                    <label asp-for="Thumb" class="control-label">Thumb</label>
                    <input asp-for="Thumb" id="addimage" class="form-control" readonly />
                </div>

                <div class="input-box">
                    <label asp-for="Title" class="control-label">Title</label>
                    <input asp-for="Title" class="form-control" readonly />
                </div>

                <div class="input-box">
                    <label asp-for="Alias" class="control-label">Alias</label>
                    <input asp-for="Alias" class="form-control" readonly />
                </div>

                <div class="input-box">
                    <label asp-for="MetaDesc" class="control-label">MetaDesc</label>
                    <input asp-for="MetaDesc" class="form-control" readonly />
                </div>

                <div class="input-box">
                    <label asp-for="MetaKey" class="control-label">MetaKey</label>
                    <input asp-for="MetaKey" class="form-control" readonly />
                </div>

                <div class="input-box">
                    <label asp-for="Cover" class="control-label">Cover</label>
                    <input asp-for="Cover" class="form-control" readonly />
                </div>

                <div class="input-box">
                    <label asp-for="SchemaMarkup" class="control-label">Schema Markup</label>
                    <input asp-for="SchemaMarkup" class="form-control" readonly />
                </div>

            </div>

        </div>

    </dl>
    <form asp-action="Delete">
        <input type="hidden" asp-for="CatId" />

        <div class="group_button">
            <input type="button"
                   onclick="window.location.href='@Url.Action("Index")'"
                   value="Back to List" class="btn btn-info btn-sm">
            <input type="submit" id="delete" value="Delete" class="btn btn-danger" />
        </div>
    </form>
</div>
